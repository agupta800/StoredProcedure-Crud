@model IEnumerable<StoredProcedure_Crud.Models.StoredModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a class="btn btn-outline-primary" asp-action="Create">Create New</a>
</p>
<table class="table tab-content sticky-top  table-bordered ">
    <thead class="table">
        <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>City</th>
        <th>PinCode</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.City</td>
                <td>@item.PinCode</td>
            </tr>
        }
    </tbody>
</table>
